    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug Main",
            "sourceLanguages": [
                "rust"
            ],
            "cargo": {
                "args": [
                    "build",
                    "--bin=slpprocess",
                    "--manifest-path=${workspaceFolder}/Cargo.toml"
                ]
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },

        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}